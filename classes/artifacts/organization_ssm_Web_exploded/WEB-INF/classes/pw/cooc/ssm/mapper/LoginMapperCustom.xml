<mapper namespace="pw.cooc.ssm.mapper.LoginMapperCustom" >

	<select id="loginAll" parameterType="java.lang.String" resultType="pw.cooc.ssm.po.Login">
		SELECT login.* FROM login where account = #{value} 
	</select>
	<select id="fingLoginAll" parameterType="java.lang.String" resultType="pw.cooc.ssm.po.Login">
		SELECT login.* FROM login where account = #{value} 
	</select>
</mapper>